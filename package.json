{
    "name": "babylon-mmd",
    "version": "0.32.0",
    "description": "babylon mmd runtime",
    "main": "esm/index.js",
    "scripts": {
        "start": "webpack serve --mode development",
        "watch": "webpack --watch",
        "build": "webpack --env production",
        "build:lib:esm:wasm": "cd src/Runtime/Optimized/wasm_src && rimraf ../wasm && wasm-pack build --target bundler --release --out-dir ../wasm --out-name index && cd ../../../.. && copyfiles -f src/Runtime/Optimized/wasm/* dist/esm/Runtime/Optimized/wasm",
        "build:lib:esm": "tspc --project tsconfig.lib.json",
        "build:lib:umd:wasm": "webpack --env production --config webpack.libwasm.config.ts",
        "build:lib:umd": "webpack --env production --config webpack.lib.config.ts && webpack --env development --config webpack.lib.config.ts",
        "build:lib": "rimraf dist && npm run build:lib:esm:wasm && npm run build:lib:esm && npm run build:lib:umd:wasm && npm run build:lib:umd",
        "publish:lib": "ts-node publishScript.ts && npm publish ./dist",
        "lint": "eslint --ext .ts,.tsx . --fix --cache"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/noname0310/babylon-mmd.git"
    },
    "author": "noname0310",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/noname0310/babylon-mmd/issues"
    },
    "homepage": "https://github.com/noname0310/babylon-mmd#readme",
    "devDependencies": {
        "@babylonjs/havok": "^1.3.0",
        "@babylonjs/inspector": "^6.30.0",
        "@babylonjs/loaders": "^6.30.0",
        "@babylonjs/materials": "^6.30.0",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "@wasm-tool/wasm-pack-plugin": "^1.7.0",
        "compression-webpack-plugin": "^10.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "copyfiles": "^2.4.1",
        "eslint": "^8.54.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-webpack-plugin": "^4.0.1",
        "file-loader": "^6.2.0",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.3",
        "rimraf": "^5.0.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.1",
        "ts-patch": "^3.0.2",
        "typescript": "^5.2.2",
        "typescript-transform-paths": "^3.4.6",
        "url": "^0.11.3",
        "wasm-pack": "^0.12.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "keywords": [
        "3D",
        "javascript",
        "html5",
        "webgl",
        "babylon.js",
        "mmd"
    ],
    "sideEffects": [
        "**/mmdOutlineRenderer.*",
        "**/pmdLoader.*",
        "**/pmxLoader.*",
        "**/bpmxLoader.*",
        "**/mmdCompositeRuntimeCameraAnimation.*",
        "**/mmdCompositeRuntimeModelAnimation.*",
        "**/mmdRuntimeCameraAnimation.*",
        "**/mmdRuntimeCameraAnimationGroup.*",
        "**/mmdRuntimeModelAnimation.*",
        "**/mmdRuntimeModelAnimationGroup.*",
        "**/index.*"
    ],
    "peerDependencies": {
        "@babylonjs/core": "^6.30.0"
    }
}
